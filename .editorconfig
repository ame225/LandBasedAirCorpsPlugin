root = true

[*.{cs,xaml}]
end_of_line = crlf
charset = utf-8

[*.cs]
dotnet_style_qualification_for_event = true:suggestion
dotnet_style_qualification_for_field = true:suggestion
dotnet_style_qualification_for_method = true:suggestion
dotnet_style_qualification_for_property = true:suggestion

[LandBasedAirCorpsPlugin/Models/Raw/**.cs]
dotnet_naming_rule.raw_rule.severity = none
dotnet_naming_rule.raw_rule.style = raw_style
dotnet_naming_rule.raw_rule.symbols = raw_symbol
dotnet_naming_style.raw_style.capitalization = camel_case
dotnet_naming_symbols.raw_symbol.applicable_kinds = property, class